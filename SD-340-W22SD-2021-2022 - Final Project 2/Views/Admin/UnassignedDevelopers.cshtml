@model IEnumerable<SD_340_W22SD_2021_2022___Final_Project_2.Models.ApplicationUser>

<ul>
    @foreach (ApplicationUser user in Model)
    {
        <li>
            @user.UserName

            <form asp-action="AssignDeveloper" asp-controller="Admin" asp-route-userId="@user.Id" method="post">
                <button type="submit">Assign Developer</button>
            </form>

            <form asp-action="AssignProjectManager" asp-controller="Admin" asp-route-userId="@user.Id" method="post">
                <button type="submit">Assign Project Manager</button>
            </form>
        </li>
    }
</ul>